{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "dev": {
      "cache": false
    },
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "db": {
      "cache": false
    },
    "db:up": {
      "cache": false
    },
    "db:down": {
      "cache": false
    }
  }
}